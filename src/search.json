---
layout: none
---
[
  {% for cmd in site.commands %}
    {
      "title"    : "{{ cmd.title | escape }}",
      "url"      : "{{ cmd.url | escape }}",
      "ancestry" : "{{ cmd.ancestry | join: ' ' }}",
      "description" : "{{ cmd.description | join: ' ' | strip_newlines | escape }}",
      "usage"    : "{{ cmd.usage | escape }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]